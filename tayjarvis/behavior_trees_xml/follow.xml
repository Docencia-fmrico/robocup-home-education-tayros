 <root main_tree_to_execute = "FollowTree" >
     <BehaviorTree ID="FollowTree">

        <ReactiveSequence name="root_sequence">
            <Set_goal name="set_goal" pos="{goal}"/>
            <Move name="goHome" pos="{goal}"/>  <!--Always true-->
            <Target_reached name="Target_reached"/>
        </ReactiveSequence>

     </BehaviorTree>
 </root>