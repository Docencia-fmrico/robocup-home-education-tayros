 <root main_tree_to_execute = "backHome" >
     <BehaviorTree ID="backHome">

        <ReactiveSequence name="root_sequence">
            <ReactiveFallback>
                <Move name="goHome" pos="{Home}"/> 
                <Recovery name="Recovery"/>
            </ReactiveFallback>
            
            <Say_data name="sayData" data="{Description}"/>
        </ReactiveSequence>

     </BehaviorTree>
 </root>